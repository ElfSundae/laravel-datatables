{
    "name": "elfsundae/laravel-datatables",
    "type": "library",
    "description": "An extended package based on yajra/laravel-datatables.",
    "keywords": ["laravel", "datatables"],
    "homepage": "https://github.com/ElfSundae/laravel-datatables",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "5.5.*",
        "yajra/laravel-datatables-oracle": "^8.3.2",
        "yajra/laravel-datatables-html": "^3.5.1",
        "yajra/laravel-datatables-buttons": "^3.1.2"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\DataTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\DataTables\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev"
        },
        "laravel": {
            "dont-discover": [
                "yajra/laravel-datatables-oracle",
                "yajra/laravel-datatables-html",
                "yajra/laravel-datatables-buttons",
                "maatwebsite/excel"
            ],
            "aliases": {
                "DataTables": "Yajra\\DataTables\\Facades\\DataTables",
                "Excel": "Maatwebsite\\Excel\\Facades\\Excel"
            }
        }
    }
}
