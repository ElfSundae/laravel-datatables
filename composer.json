{
    "name": "elfsundae/laravel-datatables",
    "type": "library",
    "description": "Laravel DataTables Helper",
    "keywords": ["laravel", "datatables"],
    "homepage": "https://github.com/ElfSundae/laravel-datatables",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "yajra/laravel-datatables-oracle": "~8.0",
        "yajra/laravel-datatables-html": "~3.1",
        "yajra/laravel-datatables-buttons": "~3.0"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\Laravel\\DataTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\Laravel\\DataTables\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "dont-discover": [
                "yajra/laravel-datatables-oracle",
                "yajra/laravel-datatables-html",
                "yajra/laravel-datatables-buttons",
                "maatwebsite/excel"
            ]
        }
    }
}
