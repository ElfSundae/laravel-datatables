{
    "name": "elfsundae/laravel-datatables",
    "type": "library",
    "description": "Custom Laravel DataTables",
    "keywords": ["laravel", "datatables"],
    "homepage": "https://github.com/ElfSundae/laravel-datatables",
    "license": "MIT",
    "authors": [
        {
            "name": "Elf Sundae",
            "email": "elf.sundae@gmail.com",
            "homepage": "https://0x123.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "yajra/laravel-datatables-oracle": "^8.3.2",
        "yajra/laravel-datatables-html": "~3.3",
        "yajra/laravel-datatables-buttons": "^3.1.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "ElfSundae\\DataTables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ElfSundae\\DataTables\\Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.4-dev"
        },
        "laravel": {
            "dont-discover": [
                "yajra/laravel-datatables-oracle",
                "yajra/laravel-datatables-html",
                "yajra/laravel-datatables-buttons",
                "maatwebsite/excel"
            ]
        }
    }
}
